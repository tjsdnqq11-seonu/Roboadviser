n","\n","          base_capital = stocks * price # 기준을 주식으로만 설정: cash x\n","          if date in backtest_df.index[1:]:  # 현재 날짜가 nasdaq 데이터에 있을 경우\n","              price = nasdaq_df.loc[date, 'price']  # 해당 날짜의 가격 가져오기\n","              mean_p = backtest_df.loc[date, f'mean_{window}']  # mean 값을 가져오기\n","              present_capital = stocks * price\n","              base_capital_up = base_capital * (1 + percent)\n","              base_capital_down = base_capital * (1 - percent)\n","\n","              # 기초자본대비 percent 이상/이하일 때 rebalancing 할지 확인\n","              if (base_capital_up <= present_capital) or (base_capital_down >= present_capital):\n","                  # mean_p >= error_std 일 경우, 자본의 %를 매수\n","                  if mean_p >= result[f'result_{window}']['error_std']*std_per1:\n","                      buy_amount = cash * per1\n","                      stock_amount = round(buy_amount / price)\n","                      stocks += stock_amount\n","                      cash -= stock_amount * price\n","                      # print(\"Date:\",date.date(),\", buy price:\",price,\", mean price:\",mean_p)\n","\n","                  # mean_p >= error_std/ 일 경우, 자본의 %를 매수\n","                  elif mean_p >= (result[f'result_{window}']['error_std']*std_per2):\n","                      buy_amount = cash * per2\n","                      stock_amount = round(buy_amount / price)\n","                      stocks += stock_amount\n","                      cash -= stock_amount * price\n","                      # print(\"Date:\",date.date(),\", buy price:\",price,\", mean price:\",mean_p)\n","\n","                  # mean_p이 <= -error_std/ 일 경우, 보유 주식의 %를 매도\n","                  elif mean_p <= -(result[f'result_{window}']['error_std']*std_per2):\n","                      sell_amount = round(stocks * per2)\n","                      stocks -= sell_amount\n","                      cash += sell_amount * price\n","                      # print(\"Date:\",date.date(),\", sell price:\",price,\", mean price:\",mean_p)\n","\n","                  # mean_p이 <= -error_std 일 경우, 보유 주식 %를 매도\n","                  elif mean_p <= -(result[f'result_{window}']['error_std']*std_per1):\n","                      sell_amount = round(stocks * per1)\n","                      stocks -= sell_amount\n","                      cash += sell_amount * price\n","                      # cash += stocks * price\n","                      # stocks = 0  # 주식 보유량 초기화\n","                      # print(\"Date:\",date.date(),\", sell price:\",price,\", mean price:\",mean_p)\n","\n","              # 포트폴리오 총 가치 (현금 + 주식 가치) 계산 후 저장\n","              total_value = cash + stocks * price\n","              portfolio_value.append(total_value)\n","\n","    # 백테스팅 종료 시점에 보유 중인 주식 전량 매도\n","    if stocks > 0:\n","        cash += stocks * price\n","        stocks = 0\n","        # print(\"Date:\",date.date(),\", final sell price:\",price)\n","\n","    # 최종 자본에 대한 수익률 계산\n","    final_return_rate = total_value / first_capital -1\n","\n","    # Benchmark index 수익률 계산\n","    start_p = float(nasdaq[nasdaq.index == backtest_df.index[0]]['price'])\n","    end_p = float(nasdaq[nasdaq.index == backtest_df.index[-1]]['price'])\n","    BM_value = end_p/start_p -1\n","\n","    return final_return_rate, portfolio_value, BM_value\n"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"elapsed":705,"status":"ok","timestamp":1689955003282,"user":{"displayName":"이하림학부생","userId":"10320128737564412082"},"user_tz":-540},"id":"9-rqPVgLadAp","outputId":"d4990f7c-3554-47cf-c045-17c8406da105"},"outputs":[{"output_type":"stream","name":"stdout","text":["최종 수익률: 52.14%\n","BM 수익률: 86.69%\n"]}],"source":["# 함수 사용\n","# percent 부터 per2까지 for문 사용해서 최고 수익률 보이는 파라미터 조합 찾기\n","final_return_rate, portfolio_value, BM_value = buysell_strategy(develop_test_df, nasdaq, 0.02, 1, 0.3, 0.5, 0.2)\n","print(f'최종 수익률: {final_return_rate * 100:.2f}%')\n","pri